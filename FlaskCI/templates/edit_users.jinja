{% extends 'base.jinja' %}

{% block content %}
<div class="content">
  <h4>Edit Users</h4>
  <p><a href="{{ url_for('create_user') }}" class="btn btn-default">Add User</a></p>
  {% for user in users %}
    <p><a href="{{ url_for('update_user', user_id = user.uuid) }}">{{ user.email }}</a></p>
  {% endfor %}
</div>
{% endblock %}